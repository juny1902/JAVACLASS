package networkTest;

import java.net.InetSocketAddress;
import java.net.Socket;

public class ClientExample {
	public static void main(String[] args){
		Socket socket = null;
		try{
			socket = new Socket();
			System.out.println("[연결 요청]");
			socket.connect(new InetSocketAddress("localhost", 5001));
		}
	}
}
