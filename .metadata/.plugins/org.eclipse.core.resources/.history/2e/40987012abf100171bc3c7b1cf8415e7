class PartialSum extends Thread {

	private long sum = 0;
	private long division[];
	public PartialSum(long start, long end, long partition){
		division = new long[partition];
		long area = (end-start)/partition;
		for(int i=0;i<partition;i++){
			division[i] = area*i;
		}
	}
	
	public long getSum() {
		return sum;
	}
	
	public void run() {
		for (int i = 1; i <= 100; i++) {
			sum+=i;
		}
	}
}

public class Test1 {
	public static void main(String[] args) {
		PartialSum s = new PartialSum(0,100000,4);
		s.start();

		try {
			s.join();
		} catch (InterruptedException e) {

		}
		System.out.println(s.getSum());
	}

}
