class TUInterface {
	private int xlimit;
	private int ylimit;

	public void TUIFlush(char c) {
		for (int i = 0; i < xlimit; i++) {
			for (int j = 0; j < ylimit; j++) {
				System.out.print(c);
			}
			System.out.println();
		}
	}
	public void setCursor(int x, int y) {
		if(x > xlimit || x < 0 || y > ylimit || y < 0){
			System.out.println("Out of bound");
		}
		else{
			for (int i = 0; i < xlimit; i++) {
				for (int j = 0; j < ylimit; j++) {
					System.out.print(" ");
				}
				System.out.println();
			}
		}
	}

	public TUInterface(int xlimit, int ylimit) {
		this.xlimit = xlimit;
		this.ylimit = ylimit;
	}

}

public class TUI {
	public static void main(String[] args) {
		TUInterface a = new TUInterface(30, 60);
		a.TUIFlush('a');
		a.setCursor(15, 15);
		System.out.print("Hello world");
	}
}
