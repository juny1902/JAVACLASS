package FileIOTest;

import java.io.File;
import java.io.IOException;

public class FileIOTest {
	public static void main(String[] args){
		File dir = new File("D:\\FileIO");
		File file = new File("D:\\test.txt");
		dir.mkdir();
		if(file.exists()){
			try {
				file.createNewFile();
			} catch (IOException e) {
				file.delete();
				file.createNewFile();
			}
		}
		
	}
}
