class test1 extends Thread{
	double t = 0;
	public void run(){
		try {
			Thread.sleep(500);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		t += 0.5;
		System.out.printf("%.2f 초 경과\n",t);
	}
}
class test2 extends Thread{
	double t = 0;
	public void run(){
		try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		t += 4;
		System.out.printf("%.2f 초 경과\n",t);
	}
}
public class groupThread {
	public static void main(String[] args){
		
	}
}
