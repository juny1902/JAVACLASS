package FileIOTest;

import java.io.File;
import java.io.IOException;

public class FileIOTest {
	public static void main(String[] args) {
		File dir = new File("D:\\FileIO");
		dir.exists()
		dir.mkdir();
		File file = new File("D:\\FileIO\\test.txt");
		if (file.exists()) {
			try {
				file.createNewFile();
			} catch (IOException e) {
				file.delete();
			}
		}
		System.out.println("생성된 파일의 경로는 : " + file.getPath());

	}
}
