class ThreadA extends Thread {

	private int sum = 0;

	@Override
	public void run() {
		for (int i = 0; i < 1000; i++) {
			sum += 1;
		}
	}

	public int getSum() {

	}
}

class ThreadB extends Thread {

	private int sum = 0;

	@Override
