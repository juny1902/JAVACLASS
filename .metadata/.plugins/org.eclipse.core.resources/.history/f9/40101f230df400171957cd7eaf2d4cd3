class test1 extends Thread{
	double t = 0;
	public void run(){
		try {
			Thread.sleep(500);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		t += 0.5;
		System.out.printf("%.2f ÃÊ °æ°ú\n",t);
	}
}
class test2 extends Thread{
	double t = 0;
	public void run(){
		try {
			Thread.sleep(2000);
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		System.out.printf("¶¯ !");
	}
}
public class groupThread {
	public static void main(String[] args){
		test1 a = new test1();
	}
}
